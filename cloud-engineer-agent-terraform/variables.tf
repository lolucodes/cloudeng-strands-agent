variable "project_name" {
  description = "Name prefix for all resources"
  type        = string
  default     = "cloud-engineer-agent"
}

variable "aws_region" {
  description = "AWS region to deploy into"
  type        = string
  default     = "us-east-1"
}

variable "vpc_cidr" {
  description = "CIDR block for the VPC"
  type        = string
  default     = "10.0.0.0/16"
}
